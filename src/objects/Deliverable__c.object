        <errorConditionFormula>NumofApproversRole__c &gt;1</errorConditionFormula>
        <errorMessage>There can only be 1 approver per deliverable. Please update the existing technical quality team approver.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EcoOnly1OwnerCheck</fullName>
        <active>true</active>
        <description>Validates that there is only 1 owner in the technical quality team</description>
