    <webLinks>
        <fullName>NewDeliverable</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>New Deliverable</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>false</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/apex.js&quot; )}
var connection = sforce.connection;
var newdeliverable = new sforce.SObject(&quot;Deliverable__c&quot;);
newdeliverable.Project__c = {!pse__Proj__c.Id};
newdeliverable.Approver__c = {!pse__Proj__c.pse__Project_ManagerId__c};
result = sforce.connection.create([newdeliverable]);
alert(result);</url>
    </webLinks>
