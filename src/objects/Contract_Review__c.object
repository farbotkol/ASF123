        <description>External Due Date can not be less then Date Review is Required</description>
        <errorConditionFormula>External_Due_Date__c &lt;  Date_Required__c</errorConditionFormula>
        <errorDisplayField>External_Due_Date__c</errorDisplayField>
        <errorMessage>&apos;External Due Date&apos; can not be less then &apos;Date Review is Required&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Federal_Contract_US_Only</fullName>
        <active>true</active>
        <errorConditionFormula>Federal_Contract__c = True &amp;&amp; (Country_Code__c &lt;&gt; &apos;US&apos; &amp;&amp; Country_Code__c &lt;&gt; &apos;CA&apos;)</errorConditionFormula>
        <errorMessage>Federal Contract can only be selected when the country is &apos;United States&apos; or &apos;Canada&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Gulf_Coast_Region_Only_temp_rule</fullName>
        <active>false</active>
        <errorConditionFormula>CONTAINS(&quot;AK:WA:OR:ID:MT:ND:SD:WY:BC:YT:NT:NU:AB:SK:MB:UT:AZ:NM:OK:TX:AR:MS:LA:CO&quot;,State_Province_Emirate__r.State_Code__c )==False</errorConditionFormula>
        <errorDisplayField>State_Province_Emirate__c</errorDisplayField>
        <errorMessage>Contract Reviews can currently only be submitted for the USA and Canada West / Gulf Coast Region.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>State_Required_for_AUS_US_CAN</fullName>
        <active>true</active>
        <description>Due to the queue assignment rules for Australian, US and Canaidian legal team queues being based off State/prov the state is required for these countries</description>
        <errorConditionFormula>CONTAINS(&quot;AU:CA:US:GB&quot;,Country_Code__c) 
&amp;&amp;   ISBLANK(State_Province_Emirate__c)</errorConditionFormula>
        <errorMessage>When the &apos;Country&apos; field is Australia, Canada, United Kingdom or United States a &apos;State/Province/Emirate&apos; is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>US_Canada_Only_temp_rule</fullName>
        <active>false</active>
        <errorConditionFormula>CONTAINS(&quot;US:CA&quot;,Country__r.Country_Code__c ) == FALSE</errorConditionFormula>
        <errorDisplayField>Country__c</errorDisplayField>
        <errorMessage>Contract Reviews can currently only be submitted for the USA and Canada West / Gulf Coast Region</errorMessage>
    </validationRules>
