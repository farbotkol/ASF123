        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/31.0/connection.js&quot;)}


  
  var answer = true;
  answer = confirm(&quot;Are you sure you want to withdraw this request.&quot;);
  

  if (answer){
    var crObj = new sforce.SObject(&quot;Contract_Review__c&quot;);
    crObj.id = &apos;{!Contract_Review__c.Id}&apos;; /* Need Id field to update Contract_Review*/
    crObj.Withdraw__c = true; /* Change status */

    /* update method takes an array of Cases; init to 1 element - &apos;caseObj&apos; */
    var result = sforce.connection.update([crObj]);

    if (result[0].success == &apos;false&apos;) {
      alert(&apos;There are validation errors, please edit and re-save this contract review.&apos;);
    }
    else {
      alert(&apos;Withdrawal completed&apos;);
      location.reload(true); /* redisplay the detail page */
    }
  }</url>
