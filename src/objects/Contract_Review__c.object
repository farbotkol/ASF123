    <webLinks>
        <fullName>Submit_to_Contract_Reviewer</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Submit Contract Review</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/31.0/connection.js&quot;)}

if (&apos;{!Contract_Review__c.Status_Review__c}&apos; == &apos;Pending Submit&apos;){
  
    // CHECK IF THERE ARE ANY ATTACHMENTS
  var resultcnt= sforce.connection.query(&quot;SELECT count() FROM Attachment WHERE ParentId = &apos;{!Contract_Review__c.Id}&apos;&quot;);
  
  var answer = true;
  if (resultcnt.size == 0 ){
      answer = confirm(&quot;You have not added any attachments, are you sure you wish to submit this review?&quot;);
  }

  if (answer){
    var crObj = new sforce.SObject(&quot;Contract_Review__c&quot;);
    crObj.id = &apos;{!Contract_Review__c.Id}&apos;; /* Need Id field to update Contract_Review*/
    crObj.Submitted__c = true; /* Change status */

    /* update method takes an array of Cases; init to 1 element - &apos;caseObj&apos; */
    var result = sforce.connection.update([crObj]);

    if (result[0].success == &apos;false&apos;) {
      alert(&apos;There are validation errors, please edit and resave this contract review.&apos;);
    }
    else {
      alert(&apos;Submission completed&apos;);
      location.reload(true); /* redisplay the detail page */
    }
  }
  
}
else{
    alert(&apos;This Contract Review has already been submitted&apos;);
}</url>
