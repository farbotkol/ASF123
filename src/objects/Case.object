    <fields>
        <fullName>SS_Category__c</fullName>
        <externalId>false</externalId>
        <formula>IF( !SS_Is_Uncategorized__c,
MID(
IF( SS_Is_Backup_Request__c,&apos;Backup Request, &apos;, null) &amp; 
IF( SS_Is_Expense_Report__c , &apos;Expense Report, &apos;, null)&amp;
IF( SS_Is_Special_Handling__c , &apos;Special Handling, &apos;, null)&amp;
IF( SS_Is_Invoice_Request__c, &apos;Invoice Request, &apos;, null)
,0, LEN(
IF( SS_Is_Backup_Request__c,&apos;Backup Request, &apos;, null) &amp; 
IF( SS_Is_Expense_Report__c , &apos;Expense Report, &apos;, null)&amp;
IF( SS_Is_Special_Handling__c , &apos;Special Handling, &apos;, null)&amp;
IF( SS_Is_Invoice_Request__c, &apos;Invoice Request, &apos;, null)&amp;
IF( SS_Is_Uncategorized__c , &apos;Uncategorized&apos;, null)
)-2),
IF( SS_Is_Uncategorized__c , &apos;Uncategorized&apos;, null)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Category</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
