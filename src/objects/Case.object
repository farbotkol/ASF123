        <fullName>SS_Is_Uncategorized__c</fullName>
        <externalId>false</externalId>
        <formula>NOT( OR( SS_Is_Backup_Request__c, SS_Is_Expense_Report__c, SS_Is_Invoice_Request__c, SS_Is_Special_Handling__c))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Is Uncategorized</label>
