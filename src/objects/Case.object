    <fields>
        <fullName>SS_SLA_Target__c</fullName>
        <description>Shared Services SLA Target Time</description>
        <externalId>false</externalId>
        <formula>IF(SS_Is_Executive__c, 24,
  IF( SS_Is_Backup_Request__c, 24,
    IF( SS_Is_Special_Handling__c, 24,
      IF( SS_Is_Expense_Report__c, 48,
        IF( ( Is_Vendor__c ||  On_Behalf_of_Vendor__c ) &amp;&amp; SS_Is_Invoice_Request__c, 48, 
          IF( SS_Is_Invoice_Request__c, 72, null
          )
        )      
      )
    )
  )
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>SLA Target (Hours)</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>SS_Time_Left__c</fullName>
        <description>Shared Services Time Left indicator</description>
        <externalId>false</externalId>
        <formula>IF(!ISNULL(SS_SLA_Target__c),
    IF(SS_SLA_Target__c - (ceiling((NOW() -  CreatedDate)*24))
       +
    (((((NOW() -  CreatedDate)*24)-ceiling((NOW() -  CreatedDate)*24))*60)/100) &gt; 0,
        SS_SLA_Target__c - (ceiling((NOW() -  CreatedDate)*24))
           +
        (((((NOW() -  CreatedDate)*24)-ceiling((NOW() -  CreatedDate)*24))*60)/100),null
    )
,null)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Time Left (Hours)</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
