    <validationRules>
        <fullName>pse__check_end_date</fullName>
        <active>true</active>
        <description>End date cannot occur before start date</description>
        <errorConditionFormula>AND(NOT(ISNULL(pse__End_Date__c)),pse__End_Date__c &lt; pse__Start_Date__c)</errorConditionFormula>
        <errorDisplayField>pse__End_Date__c</errorDisplayField>
        <errorMessage>End date cannot occur before start date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>pse__only_one_source_allowed</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
  AND(LEN(pse__Region__c)&gt;0, 
    OR(LEN(pse__Practice__c)&gt;0,LEN(pse__Group__c)&gt;0,LEN(pse__Project__c)&gt;0) ),
  AND(LEN(pse__Practice__c)&gt;0, 
    OR(LEN(pse__Region__c)&gt;0,LEN(pse__Group__c)&gt;0,LEN(pse__Project__c)&gt;0) ),
  AND(LEN(pse__Group__c)&gt;0, 
    OR(LEN(pse__Region__c)&gt;0,LEN(pse__Practice__c)&gt;0,LEN(pse__Project__c)&gt;0) ),
  AND(LEN(pse__Project__c)&gt;0, 
    OR(LEN(pse__Region__c)&gt;0,LEN(pse__Group__c)&gt;0,LEN(pse__Group__c)&gt;0) )
)</errorConditionFormula>
        <errorMessage>Only one of Region, Practice, Group, or Project may be specified.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>pse__source_required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISBLANK(pse__Region__c), ISBLANK(pse__Practice__c), ISBLANK(pse__Group__c), ISBLANK(pse__Project__c))</errorConditionFormula>
        <errorMessage>One of Region, Practice, Group, or Project is required</errorMessage>
    </validationRules>
