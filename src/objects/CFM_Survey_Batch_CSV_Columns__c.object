<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <customSettingsVisibility>Protected</customSettingsVisibility>
    <description>This custom setting manages columns and mappings for the CFM Survey Batch Project CSV generation</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Column_Field__c</fullName>
        <description>API Name of the field (can include __r relationships) to use to get the data for this column, referenced from CFM_Survey_Batch_Project__c</description>
        <externalId>false</externalId>
        <label>Column Field</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Column_Label__c</fullName>
        <externalId>false</externalId>
        <label>Column Label</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Index__c</fullName>
        <description>Index of column in CSV - the lowest index is the leftmost column</description>
        <externalId>false</externalId>
        <label>Index</label>
        <precision>5</precision>
        <required>true</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>true</unique>
    </fields>
    <fields>
        <fullName>Is_Custom_Implementation__c</fullName>
        <defaultValue>false</defaultValue>
        <description>Denotes if this column required special apex code to generate the value</description>
        <externalId>false</externalId>
        <label>Is Custom Implementation</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Is_Email_Column__c</fullName>
        <defaultValue>false</defaultValue>
        <description>Denotes if this column is the Email column, which is required</description>
        <externalId>false</externalId>
        <label>Is Email Column</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Is_Language_Column__c</fullName>
        <defaultValue>false</defaultValue>
        <description>Checked if this is the column that Qualtrics should examine to determine the survey language.</description>
        <externalId>false</externalId>
        <label>Is Language Column</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <label>CFM Survey Batch Project CSV Columns</label>
</CustomObject>
