    <fields>
        <fullName>ColumnHeader__c</fullName>
        <externalId>false</externalId>
        <formula>BLANKVALUE(IF(ISBLANK(
   IF(ISBLANK( ProjectTeamMember__c )
   ,
   ResourceName__c
   ,
   ProjectTeamMember__r.Contact__r.FirstName + &apos; &apos; + ProjectTeamMember__r.Contact__r.LastName
   )
)
,
SubsAndVendors__r.VendorName__c
,
IF(ISBLANK( ProjectTeamMember__c )
   ,
   ResourceName__c
   ,
   ProjectTeamMember__r.Contact__r.FirstName + &apos; &apos; + ProjectTeamMember__r.Contact__r.LastName
   )
