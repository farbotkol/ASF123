    <fields>
        <fullName>AppliedBillRate__c</fullName>
        <externalId>false</externalId>
        <formula>IF( ISPICKVAL(MarginType__c , &apos;Fixed&apos;),  OverrideBillRate__c , 
IF( ISPICKVAL(MarginType__c , &apos;Amount&apos;),  OverrideBillRate__c ,  OverrideUplift__c *  CostRate__c))</formula>
