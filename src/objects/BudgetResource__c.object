    <validationRules>
        <fullName>Cannot_have_both_resource_and_PTA</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
 NOT(ISBLANK(ProjectTeamMember__c ))
,
 NOT(ISBLANK(ResourceName__c ))
)</errorConditionFormula>
        <errorMessage>Cannot have both a Project Team Member and a Resource Name.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_Resource_or_PTA</fullName>
        <active>false</active>
        <errorConditionFormula>AND(AND(
 ISBLANK(ProjectTeamMember__c )
,
 ISBLANK(ResourceName__c )
), ISBLANK(SubsAndVendors__c))</errorConditionFormula>
        <errorMessage>Either Project Team Member, Vendor or Resource Name is required.</errorMessage>
    </validationRules>
