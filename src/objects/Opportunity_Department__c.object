        <fullName>BdgEng_Sub_Practice_Area_required</fullName>
        <active>false</active>
        <description>Requires that Building Engineering opps have an entry in the sub practice area field.
Turned off 10/2014 when all D+P Sub Practices were removed.</description>
        <errorConditionFormula>AND(NOT(CONTAINS( $User.CommunityNickname, &apos;SystemNA&apos;)) ,
NOT(CONTAINS( $Profile.Name, &apos;API Access&apos;)), 
 CONTAINS(  Business_Line__c  , &apos;Building Engineering&apos;)
, 
ISNULL( LastModifiedDate) ,

OR(
BEGINS(TEXT(  Opportunity__r.StageName  ),&quot;1&quot;),
BEGINS(TEXT( Opportunity__r.StageName),&quot;2&quot;),
BEGINS(TEXT( Opportunity__r.StageName),&quot;3&quot;),
BEGINS(TEXT( Opportunity__r.StageName),&quot;5&quot;)
)
,
ISBLANK(   Sub_Practice_Area__c 
 ))</errorConditionFormula>
        <errorDisplayField>split__c</errorDisplayField>
        <errorMessage>Please select a Sub Practice Area for the Building Engineering Department</errorMessage>
    </validationRules>
