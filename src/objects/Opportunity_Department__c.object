        <fullName>OD_Forecast_Revenue_Month_10__c</fullName>
        <description>Displays forecast of net revenue for the 10th month after today&apos;s month</description>
        <externalId>false</externalId>
        <formula>IF ( 

(MONTH (TODAY())&gt;2 
&amp;&amp; 
(YEAR (Opportunity__r.Estimated_work_start_date__c) = YEAR (TODAY()) 
|| (MONTH (Opportunity__r.Estimated_work_start_date__c) &lt;= MONTH (TODAY()) - 2 &amp;&amp; YEAR (Opportunity__r.Estimated_work_start_date__c) = YEAR (TODAY()) +1 
)) 
&amp;&amp; 
((YEAR (Opportunity__r.Work_end_date_del__c ) &gt; YEAR (TODAY()) + 1 
|| ( (YEAR (Opportunity__r.Work_end_date_del__c ) = YEAR (TODAY()) + 1 &amp;&amp; MONTH (Opportunity__r.Work_end_date_del__c ) &gt;= MONTH (TODAY()) - 2) ))) 
) 

|| 
( 
MONTH (TODAY()) &lt;=2 
&amp;&amp; YEAR (Opportunity__r.Estimated_work_start_date__c) = YEAR (TODAY()) 
&amp;&amp; MONTH (Opportunity__r.Estimated_work_start_date__c) &lt;= MONTH (TODAY()) +10 
&amp;&amp; 
((YEAR (Opportunity__r.Work_end_date_del__c ) &gt; YEAR (TODAY()) 
|| ((YEAR (Opportunity__r.Work_end_date_del__c ) = YEAR (TODAY()) &amp;&amp; MONTH (Opportunity__r.Work_end_date_del__c ) &gt;= MONTH (TODAY()) +10)))) 
), 
( TO_Project_Forecast_Share__c / 
( 
((YEAR(Opportunity__r.Work_end_date_del__c ) - YEAR(Opportunity__r.Estimated_work_start_date__c ) - 1) *12) + (12 - MONTH(Opportunity__r.Estimated_work_start_date__c) +1) + MONTH(Opportunity__r.Work_end_date_del__c)) 
) 

,0)</formula>
