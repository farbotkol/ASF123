        <description>Uses ZIP codes to determine if opp or client is in one of AECOM&apos;s metro areas of focus</description>
        <externalId>false</externalId>
        <formula>IF (BEGINS(Account.Zip__c, &quot;94&quot;) , &quot;Bay Area&quot;,
IF (BEGINS(Account.Zip__c, &quot;95&quot;) , &quot;Bay Area&quot;,
IF (BEGINS(Account.Zip__c, &quot;20&quot;) , &quot;Washington, DC&quot;,
IF (BEGINS(Account.Zip__c, &quot;22&quot;) , &quot;Washington, DC&quot;,
IF (BEGINS(Account.Zip__c, &quot;23&quot;) , &quot;Washington, DC&quot;,
IF (BEGINS(Account.Zip__c, &quot;60&quot;) , &quot;Chicago&quot;,
IF (BEGINS(Account.Zip__c, &quot;60&quot;) , &quot;Chicago&quot;,
IF (BEGINS(Account.Zip__c, &quot;L&quot;) , &quot;Toronto&quot;,
IF (BEGINS(Account.Zip__c, &quot;M&quot;) , &quot;Toronto&quot;,
IF (BEGINS(Account.Zip__c, &quot;77&quot;) , &quot;Houston&quot;,
IF (BEGINS(Account.Zip__c, &quot;33&quot;) , &quot;Miami&quot;,
IF (BEGINS(Account.Zip__c, &quot;30&quot;) , &quot;Atlanta&quot;,
IF (BEGINS(Account.Zip__c, &quot;31&quot;) , &quot;Atlanta&quot;,
IF (BEGINS(Account.Zip__c, &quot;39&quot;) , &quot;Atlanta&quot;,
IF (BEGINS(Account.Zip__c, &quot;1&quot;) , &quot;New York&quot;,
IF (BEGINS(Account.Zip__c, &quot;0&quot;) , &quot;New York&quot;,
IF (BEGINS(Account.Zip__c, &quot;90&quot;) , &quot;Los Angeles&quot;,
IF (BEGINS(Account.Zip__c, &quot;91&quot;) , &quot;Los Angeles&quot;,
IF (BEGINS(Account.Zip__c, &quot;93&quot;) , &quot;Los Angeles&quot;,
IF (CONTAINS (Project_City__c, &quot;Washington&quot;),&quot;Washington, DC&quot;,
IF (CONTAINS (Project_City__c, &quot;New York&quot;),&quot;New York&quot;,
IF (CONTAINS (Project_City__c, &quot;Chicago&quot;),&quot;Chicago&quot;,
IF (CONTAINS (Project_City__c, &quot;Miami&quot;),&quot;Miami&quot;,
IF (CONTAINS (Project_City__c, &quot;Houston&quot;),&quot;Houston&quot;,
IF (CONTAINS (Project_City__c, &quot;Toronto&quot;),&quot;Toronto&quot;,
