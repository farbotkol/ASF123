    <validationRules>
        <fullName>AECOM_Contract_Revenue_more_than_0</fullName>
        <active>true</active>
        <description>Requires user to enter a contract revenue, disallows 0. 
DOES NOT WORK with Convert to Program button (causes error) - deactivatedross AECOM Contract Revenue must be greater than 0</description>
        <errorConditionFormula>AND( NOT(CONTAINS( $User.CommunityNickname, &apos;SystemNA&apos;)) , 
NOT(CONTAINS( $Profile.Name, &apos;API Access&apos;)), 
NOT(CONTAINS( $User.CommunityNickname, &apos;APIC&apos;)), 
Amount &lt; 1, 
NOT (CONTAINS (RecordTypeText__c, &apos;Program&apos;)), 
NOT(ISCHANGED(RecordTypeText__c)),

OR(BEGINS(TEXT( StageName ), &quot;2&quot;),BEGINS(TEXT( StageName ), &quot;3&quot;),BEGINS(TEXT( StageName ), &quot;5&quot;) ,BEGINS(TEXT( StageName ), &quot;1&quot;)) 
)</errorConditionFormula>
        <errorDisplayField>Amount</errorDisplayField>
        <errorMessage>Gross AECOM Contract Revenue must be greater than 0</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Arch_DPE_Fields_Required</fullName>
        <active>false</active>
        <description>NOT IN USE 10/2013
Replaces part of original PDD_Fields_Required validation rule.
PDD General Market Sector is required if Primary Business Line = BP&amp;E or Architecture and Stage is any value from 1 to 6</description>
        <errorConditionFormula>AND( NOT(CONTAINS( $User.CommunityNickname, &apos;SystemNA&apos;)) ,
NOT(CONTAINS( $User.CommunityNickname, &apos;APIC&apos;)),


