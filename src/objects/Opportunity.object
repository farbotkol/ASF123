IF (BEGINS(TEXT( StageName ),&quot;3&quot;), &quot;3 Submitted&quot;,
IF (BEGINS(TEXT( StageName ),&quot;4&quot;), &quot;4 On hold&quot;,
IF (BEGINS(TEXT( StageName ),&quot;5&quot;), &quot;5 SNB&quot; ,
IF (BEGINS(TEXT( StageName ),&quot;6&quot;), &quot;6 Contracted&quot;,
IF (BEGINS(TEXT( StageName ),&quot;7&quot;), &quot;7 Cancelled&quot;,
IF (BEGINS(TEXT( StageName), &quot;8&quot;), &quot;8 No-Go&quot;,
 &quot;9 Lost&quot;))))))))</formula>
