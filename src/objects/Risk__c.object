    <fields>
        <fullName>RiskScore__c</fullName>
        <externalId>false</externalId>
        <formula>IF(AND(ISPICKVAL(LikelihoodofOccurrence__c,&quot;Possible&quot;),ISPICKVAL(RiskConsequence__c,&quot;Moderate&quot;)),15,
IF(AND(ISPICKVAL(LikelihoodofOccurrence__c,&quot;Possible&quot;),ISPICKVAL(RiskConsequence__c,&quot;Major&quot;)),20,
IF(AND(ISPICKVAL(LikelihoodofOccurrence__c,&quot;Possible&quot;),ISPICKVAL(RiskConsequence__c,&quot;Catastrophic&quot;)),25,
IF(AND(ISPICKVAL(LikelihoodofOccurrence__c,&quot;Likely&quot;),ISPICKVAL(RiskConsequence__c,&quot;Moderate&quot;)),30,
IF(AND(ISPICKVAL(LikelihoodofOccurrence__c,&quot;Likely&quot;),ISPICKVAL(RiskConsequence__c,&quot;Major&quot;)),40,
IF(AND(ISPICKVAL(LikelihoodofOccurrence__c,&quot;Likely&quot;),ISPICKVAL(RiskConsequence__c,&quot;Catastrophic&quot;)),50,
IF(AND(ISPICKVAL(LikelihoodofOccurrence__c,&quot;Probable&quot;),ISPICKVAL(RiskConsequence__c,&quot;Moderate&quot;)),60,
IF(AND(ISPICKVAL(LikelihoodofOccurrence__c,&quot;Probable&quot;),ISPICKVAL(RiskConsequence__c,&quot;Major&quot;)),80,
IF(AND(ISPICKVAL(LikelihoodofOccurrence__c,&quot;Probable&quot;),ISPICKVAL(RiskConsequence__c,&quot;Catastrophic&quot;)),100,null)))))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Risk Score</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>RiskStatus__c</fullName>
        <externalId>false</externalId>
        <formula>IF(OR(ISPICKVAL(LikelihoodofOccurrence__c, &quot;&quot;),ISPICKVAL(RiskConsequence__c, &quot;&quot;)),IMAGE(&apos;/resource/GrayDash&apos;,&apos;&apos;,30,30),
IF(AND(ISPICKVAL(LikelihoodofOccurrence__c, &quot;Possible - Could happen and has occurred on other projects&quot;),ISPICKVAL(RiskConsequence__c, &quot;Moderate - Could impact project profitability by less than $100k&quot;)), IMAGE(&apos;/resource/greenStar&apos;,&apos;&apos;,30,30),
IF(AND(ISPICKVAL(LikelihoodofOccurrence__c, &quot;Probable - Could easily happen and has occurred on previous projects more than once&quot;),ISPICKVAL(RiskConsequence__c, &quot;Major - Could impact project profitability by $100k-500k&quot;)),IMAGE(&apos;/resource/redTriangle&apos;,&apos;&apos;,30,30),
IF(ISPICKVAL(RiskConsequence__c, &quot;Catastrophic - Could impact project profitability by over $500k&quot;),IMAGE(&apos;/resource/redTriangle&apos;,&apos;&apos;,30,30), IMAGE(&apos;/resource/yellowCircle&apos;,&apos;&apos;,30,30)
))))</formula>
